    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "blob-stream": "^0.1.2",
    "brace": "^0.2.1",
    "brfs": "~1.0.1",
    "browserify": "^3.39.0",
    "codemirror": "~3.20.0",
    "coffee-script": ">=1.0.1",
    "coffeeify": "^0.6.0",
    "eslint": "^5.3.0",
    "exorcist": "^0.1.5",
    "iconv-lite": "^0.4.13",
